{
  "title": "Gitbook使用入门",
  "author": "Ein Verne",
  "description": "Gitbook使用介绍",
  "keywords": "Gitbook使用手册,markdown,gitbook,",
  "introduction": {
    "path": "README.md",
    "title": "Gitbook使用入门概述"
  },
  "plugins": [
    "editlink",
    "image-captions",
    "splitter",
    "expandable-chapters-small",
    "-mathjax",
    "ga",
    "disqus",
    "-lunr",
    "-search",
    "search-pro",
    "sitemap",
    "localized-footer"
  ],
  "pluginsConfig": {
    "theme-default": {
      "showLevel": true
    },
    "editlink": {
      "base": "https://github.com/einverne/gitbook-tutorial",
      "label": "编辑本页",
      "multilingual": false
    },
    "image-captions": {
      "caption": "Image - _CAPTION_",
      "align": "center"
    },
    "expandable-chapters-small": {},
    "fontSettings": {
      "theme": "white",
      "family": "serif",
      "size": 2
    },
    "ga": {
      "token": "UA-60753376-1"
    },
    "disqus": {
      "shortName": "gitbook-tutorial"
    },
    "search-pro": {
      "cutWordLib": "nodejieba"
    },
    "sitemap": {
      "hostname": "https://einverne.github.io/"
    },
    "localized-footer": {
      "hline": true,
      "filename": "./FOOTER.md"
    }
  },
  "links": {
    "home": false,
    "about": false,
    "issues": false,
    "contribute": false,
    "sidebar": {
      "博客地址": "https://einverne.github.io/"
    },
    "tail": {
      "Gitbook使用入门": "http://einverne.github.io/gitbook-tutorial"
    },
    "gitbook": false,
    "sharing": {
      "google": true,
      "facebook": true,
      "twitter": true,
      "weibo": true,
      "qrcode": true
    }
  },
  "pdf": {
    "toc": true,
    "pageNumbers": true,
    "fontSize": 12,
    "paperSize": "a4",
    "margin": {
      "right": 62,
      "left": 62,
      "top": 36,
      "bottom": 36
    }
  }
}